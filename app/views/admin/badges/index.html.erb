<div class="col-10 mt-0 p-1 ml-0">
  <div class="card m-1 container_auto" style="">
    <header class="header">
      <h1 class="hbigger m-3">Badge</h1>
    </header>
    <div class="container-fluid">
      <div class = 'row'>
        <div class="col-2 m-3 p-1">
          <%= link_to "New Badge", new_admin_badge_path, class: "btn btn-primary btn-block"%>
        </div>
      </div>
      <div class = 'row'>
        <table class='table m-3'>
          <thead>
            <tr>
              <th width="150"> <%= link_to "Id", admin_badges_path(order:"id"), class: "category" %></th>
              <th width="150">  <%= link_to "Name", admin_badges_path(order:"name"), class: "category" %></th>
              <th width="150"> Message </th>
              <th width="150"> Edit </th>
              <th width="150"> Delete </th>
            </tr>
          </thead>

        <% @badges.each do |badge|%>
          <tbody>
            <tr>
              <td><%= badge.id%></td>
              <td><%= badge.attendee.user.name%></td>
              <td><%= badge.notifications.last.message_sent unless badge.notifications.last.blank? %></td>
              <td><%= link_to "Edit", edit_admin_badge_path(id: badge.id), class: "category" %></td>
              <td><%= link_to 'Delete',{ action: :destroy, id: badge.id }, method: :delete , data: { confirm: 'Are you sure?' }, class: "category" %></td>
            </tr>
          </tbody>
          <%end%>
        </table>
      </div>
    </div>
  </div>
</div>
