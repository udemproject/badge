      <%= link_to "Locations", admin_locations_path, class: " nav-link left"%>
    </li>
    <li class ="nav-item col-12"></li>
    <%= link_to "Attendes", admin_attendees_path, class: "nav-link "%>
  </li>
  <li class ="nav-item col-12"></li>
  <%= link_to "Profiles", admin_profiles_path, class: "nav-link "%>
</li>
